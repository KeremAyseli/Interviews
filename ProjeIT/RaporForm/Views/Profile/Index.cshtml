@model RaporForm.Models.RaporFormDB.Employe

@{
    <div class="row">
        <div class="col-md-6">
            <div class="card" style="width: 18rem;">
                <img class="card-img-top" src="@Model.EmployeTypeImgUrl" alt="Card image cap">
                <div class="card-body">
                    <p class="card-text">@Model.Name&nbsp;@Model.Surname</p>
                </div>
            </div>
        </div>
        <div class="col-md-6">
            <h1>Raporlar</h1>
        </div>
    </div>
}
